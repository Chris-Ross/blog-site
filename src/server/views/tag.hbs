<main>
    <section>
        <h1>{{tag.tag}}</h1>
        <ul>
            {{#each tag.posts}}
            <a href="/posts/{{_id}}">
                <li>{{title}}</li>
                <li><img src="{{imageUrl}}" alt="Blog Picture"></li>
            </a>
            {{/each}}
        </ul>
    </section>
</main>